// UVFunc:11
	vec2 n_in11p1 = vec2(1.00000, 1.00000);
	vec2 n_out11p0 = n_out10p0 * n_in11p1 + UV;


// Texture2D:5
	vec4 n_out5p0 = texture(tex_vtx_5, n_out11p0);
	float n_out5p1 = n_out5p0.r;


// FloatOp:7
	float n_in7p0 = 1.00000;
	float n_out7p0 = n_in7p0 - n_out5p1;


// FloatParameter:12
	float n_out12p0 = Cloud_Intensity;


// FloatOp:8
	float n_out8p0 = n_out7p0 * n_out12p0;


// VectorOp:6
	vec3 n_out6p0 = n_out3p0 * vec3(n_out8p0);


// VectorOp:4
	vec3 n_out4p0 = n_out2p0 + n_out6p0;


// Output:0
	VERTEX = n_out4p0;


}

void fragment() {
// Texture2D:2
	vec4 n_out2p0 = texture(tex_frg_2, UV);
	float n_out2p1 = n_out2p0.r;


// Texture2D:3
	vec4 n_out3p0 = texture(tex_frg_3, vec2(n_out2p1));


// Input:16
	vec4 n_out16p0 = COLOR;
	float n_out16p4 = n_out16p0.a;


// Texture2D:13
	vec4 n_out13p0 = texture(tex_frg_13, UV);


// SmoothStep:14
	float n_in14p1 = 1.00000;
	float n_out14p0 = smoothstep(n_out16p4, n_in14p1, n_out13p0.x);


	float n_out18p0;
// ProximityFade:18
	float n_in18p0 = 1.00000;
	{
		float __depth_tex = texture(depth_tex_frg_18, SCREEN_UV).r;
		vec4 __depth_world_pos = INV_PROJECTION_MATRIX * vec4(SCREEN_UV * 2.0 - 1.0, __depth_tex, 1.0);
		__depth_world_pos.xyz /= __depth_world_pos.w;
		n_out18p0 = clamp(1.0 - smoothstep(__depth_world_pos.z + n_in18p0, __depth_world_pos.z, VERTEX.z), 0.0, 1.0);
	}


// FloatOp:17
	float n_out17p0 = n_out14p0 * n_out18p0;


// ColorParameter:4
	vec4 n_out4p0 = Explosion_Color;


// FloatConstant:6
	float n_out6p0 = 20.000000;


// VectorOp:5
	vec3 n_out5p0 = vec3(n_out4p0.xyz) * vec3(n_out6p0);


// Input:15
	vec4 n_out15p0 = COLOR;
	float n_out15p1 = n_out15p0.r;


// Texture2D:7
	vec4 n_out7p0 = texture(tex_frg_7, UV);
	float n_out7p1 = n_out7p0.r;


// SmoothStep:9
	float n_in9p1 = 1.00000;
	float n_out9p0 = smoothstep(n_out15p1, n_in9p1, n_out7p1);


// VectorOp:8
	vec3 n_out8p0 = n_out5p0 * vec3(n_out9p0);


// Output:0
	ALBEDO = vec3(n_out3p0.xyz);
	ALPHA = n_out17p0;
	EMISSION = n_out8p0;


}
